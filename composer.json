{
    "name": "getdkan/json-schema-provider",
    "description": "A simple class that validates and provides JSON schemas.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "JsonSchemaProvider\\": "src/",
            "JsonSchemaProviderTest\\": "test/"
        }
    },
    "require": {
        "justinrainbow/json-schema": "^5.2",
        "getdkan/contracts": "~0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
	"bugatino/phpcs-git-pre-commit": "dev-master",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "scripts": {
        "post-install-cmd": [
            "sh ./vendor/bugatino/phpcs-git-pre-commit/src/setup.sh"
        ],
        "post-update-cmd": [
            "sh ./vendor/bugatino/phpcs-git-pre-commit/src/setup.sh"
        ]
    },
